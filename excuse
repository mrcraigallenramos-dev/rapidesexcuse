    <script>
        function handleFocus(element) {
            // Clear placeholder text on focus
            if (element.value === element.placeholder || element.classList.contains('placeholder-active')) {
                element.value = '';
                element.classList.remove('placeholder-active');
                element.style.color = '#000';
                element.style.fontStyle = 'normal';
            }
        }

        function handleBlur(element) {
            // Restore placeholder if empty
            if (element.value.trim() === '') {
                element.value = element.placeholder;
                element.classList.add('placeholder-active');
                element.style.color = '#9CA3AF';
                element.style.fontStyle = 'italic';
            }
        }

        // Initialize placeholders on page load
        document.addEventListener('DOMContentLoaded', function() {
            const inputs = document.querySelectorAll('input[type="text"]:not([readonly]), textarea');
            inputs.forEach(function(input) {
                if (input.value.trim() === '') {
                    input.value = input.placeholder;
                    input.classList.add('placeholder-active');
                    input.style.color = '#9CA3AF';
                    input.style.fontStyle = 'italic';
                }
            });
        });

        // Clear placeholder text before printing
        window.addEventListener('beforeprint', function() {
            const inputs = document.querySelectorAll('input[type="text"]:not([readonly]), textarea');
            inputs.forEach(function(input) {
                if (input.classList.contains('placeholder-active')) {
                    input.value = '';
                }
            });
        });

        // Restore placeholder text after printing
        window.addEventListener('afterprint', function() {
            const inputs = document.querySelectorAll('input[type="text"]:not([readonly]), textarea');
            inputs.forEach(function(input) {
                if (input.value.trim() === '') {
                    input.value = input.placeholder;
                    input.classList.add('placeholder-active');
                    input.style.color = '#9CA3AF';
                    input.style.fontStyle = 'italic';
                }
            });
        });
    </script>

